{
  "name": "@mackerelio/create-release-pull-request-action",
  "version": "0.0.0",
  "private": true,
  "description": "create release pull request action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier --check src",
    "lint-fix": "run-p lint-fix:*",
    "lint-fix:eslint": "eslint src --fix",
    "lint-fix:prettier": "prettier --write src",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mackerelio/mackerel-create-release-pull-request-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^2.0.2",
    "@actions/exec": "^3.0.0",
    "@actions/github": "^5.1.1",
    "@actions/glob": "^0.5.0",
    "@actions/io": "^2.0.0",
    "ajv": "^8.11.0",
    "date-fns": "^4.1.0",
    "escape-string-regexp": "^4.0.0",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.10",
    "@types/semver": "^7.7.1",
    "@types/tmp": "^0.2.6",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.52.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.12.1",
    "globals": "^17.1.0",
    "jest": "^30.2.0",
    "js-yaml": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "3.8.1",
    "snapshot-diff": "^0.10.0",
    "timezone-mock": "^1.4.0",
    "tmp-promise": "^3.0.3",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
